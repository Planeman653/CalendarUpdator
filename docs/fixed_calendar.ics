	<script type="text/javascript">
		function showDetails() {
			var el = document.getElementById("details");
			if (el) el.className = "";

			var compassDeets = document.getElementById("compassEnv");
			if (compassDeets) compassDeets.className = "";
		}
		
		window.onload = function loadSupportEmail() {
			var supportEmailLink = document.getElementById('supportEmail');
			if (!supportEmailLink) return;
			
			var browserTz = Intl.DateTimeFormat().resolvedOptions().timeZone;
			
			var supportUrl;
			switch (browserTz){
				case 'Europe/Dublin':
				case 'Europe/London':	
					supportUrl = 'help@compass.education';
					break;
				default:
					supportUrl = 'support@compass.education';
			}
			
			supportEmailLink.innerText = supportUrl;
			supportEmailLink.href = 'mailto:' + supportUrl;	
		}
	</script>

